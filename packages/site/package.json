    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gatsby": "^4.25.7",
    "gatsby-plugin-manifest": "^4.25.0",
